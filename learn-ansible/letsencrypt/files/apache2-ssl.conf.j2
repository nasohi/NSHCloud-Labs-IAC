<VirtualHost *:{{ http_port }}>
   ServerAdmin webmaster@nshcloudlabs.com
   ServerName {{ http_host }}
   ServerAlias www.{{ http_host }}
   DocumentRoot /var/www/{{ http_host }}/{{ http_directory }}
   SSLEngine on
   SSLCertificateFile    /etc/letsencrypt/certs/nshcloudlabs.com.crt
   SSLCertificateKeyFile /etc/letsencrypt/keys/nshcloudlabs.com.key
   ErrorLog ${APACHE_LOG_DIR}/error.log
   CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
